digraph {
    and[label="Rule.And" class="operator" style="invis"]
    eq1[label="Rule.Eq" class="operator" style="invis"]
    eq2[label="Rule.Eq" class="operator" style="invis"]
    categoryId[label="Rule.Var(\"categoryId\")" class="value" style="invis"]
    price[label="Rule.Var(\"price\")" class="value" style="invis"]
    categoryValue[label="Rule.Num(10)" class="value" style="invis"]
    priceValue[label="Rule.Num(3)" class="value" style="invis"]

    and -> eq1[style="invis"]
    and -> eq2[style="invis"]

    eq1 -> categoryId[style="invis"]
    eq1 -> categoryValue[style="invis"]

    eq2 -> price[style="invis"]
    eq2 -> priceValue[style="invis"]
}
